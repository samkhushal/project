{
  "manifest_version": 3,
  "name": "HR-Skillunt Bookmark Extension",
  "version": "1.0",
  "description": "Save bookmarks based on interest",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
